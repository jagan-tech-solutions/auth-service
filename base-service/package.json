{
  "name": "egov-bff-service",
  "version": "0.0.1",
  "private": true,
  "description": "Backend For Frontend service",
  "main": "index.js",
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "scripts": {
    "ls":"ls",
    "old:start": "node ./server/bin/www",
    "test": "eslint src",
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "build:prod": "npm-run-all clean transpile:prod",
    "server": "node ./dist/bin/www",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build:prod server",
    "transpile:prod": "babel ./server --out-dir dist",
    "transpile": "babel ./src/server --out-dir dist",
    "watch:dev": "nodemon && yarn dev",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "async": "3.2.3",
    "axios": "^1.3.4",
    "base64-stream": "1.0.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.7.1",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-async-handler": "^1.1.4",
    "form-data": "^2.5.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonpath": "^1.0.2",
    "kafka-node": "^5.0.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "mustache": "^3.0.1",
    "nodemon": "^2.0.21",
    "moment-timezone": "^0.5.28",
    "morgan": "~1.9.1",
    "node-cache": "5.1.2",
    "npm-run-all": "^4.1.5",
    "pdf-merger-js": "3.2.1",
    "pg": "^8.7.1",
    "request": "^2.88.0",
    "resource-router-middleware": "^0.6.0",
    "rimraf": "^4.4.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^3.1.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "author": "Jagankumar <jagan.kumar@egov.org.in>",
  "license": "MIT"
}
